{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript", 
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules":{
        "indent":"off",
        "@typescript-eslint/indent": "error"
    },
    "overrides": [
        {
          "files": ["*.tsx", "*.ts"]
        }]
}
